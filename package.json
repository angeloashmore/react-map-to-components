{
  "name": "react-map-to-components",
  "version": "0.1.1",
  "description": "React component to map a list of data to a component based on its type",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": "git@github.com:angeloashmore/react-map-to-components.git",
  "keywords": [
    "react",
    "component"
  ],
  "author": "Angelo Ashmore",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.2.0",
    "jest": "^23.3.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "4.0.0-beta.7"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
